
<!DOCTYPE html>
<html lang="en" style="height:100%; width: 100% ">
<head>
    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
</head>
<body style="width:100%">
    
    <br />

    <div class="jumbotron" style="border: 1px inset #66c2c2; margin: auto 10% auto 10%; background-color:transparent">
        <div class="container">
            <h1> <center> Lista de Asociaciones  </center></h1>

            <br /> <br />


            <div class="row shadow-lg p-3 mb-5 bg-white rounded">

                <div class="col-sm-6">
                    <div class="input-group mb-3 col-sm-9">
                        <div class="input-group-prepend">
                            <span class="input-group-text " style="width:90px" id="inputGroup-sizing-default">Nombre</span>

                        </div>
                        <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">

                    </div>


                    <div class="input-group mb-3 col-sm-9">
                        <div class="input-group-prepend">
                            <label class="input-group-text" for="inputGroupSelect01">Provincia</label>
                        </div>
                        <select class="custom-select" id="inputGroupSelect01">
                            <option selected>Choose...</option>
                            <option value="1">One</option>
                            <option value="2">Two</option>
                            <option value="3">Three</option>
                        </select>
                    </div>


                    <div class="input-group mb-3 col-sm-9">
                        <div class="input-group-prepend">
                            <label class="input-group-text" style="width:90px" for="inputGroupSelect01">Canton</label>
                        </div>
                        <select class="custom-select" id="inputGroupSelect01">
                            <option selected>Choose...</option>
                            <option value="1">One</option>
                            <option value="2">Two</option>
                            <option value="3">Three</option>
                        </select>
                    </div>

                    <div class="input-group mb-3 col-sm-9">
                        <div class="input-group-prepend">
                            <label class="input-group-text" style="width:90px" for="inputGroupSelect01">Distrito</label>
                        </div>
                        <select class="custom-select" id="inputGroupSelect01">
                            <option selected>Choose...</option>
                            <option value="1">One</option>
                            <option value="2">Two</option>
                            <option value="3">Three</option>
                        </select>
                    </div>
                </div>


                <div class="col-sm-6">

                    <div class="input-group mb-3 col-sm-9">
                        <div class="input-group-prepend">
                            <span class="input-group-text" style="width:90px" id="inputGroup-sizing-default">Registro</span>
                        </div>
                        <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                    </div>


                    <div class="input-group mb-3 col-sm-9">
                        <div class="input-group-prepend">
                            <label class="input-group-text" style="width:90px" for="inputGroupSelect01">Región</label>
                        </div>
                        <select class="custom-select" id="inputGroupSelect01">
                            <option selected>Choose...</option>
                            <option value="1">One</option>
                            <option value="2">Two</option>
                            <option value="3">Three</option>
                        </select>
                    </div>


                    <div class="input-group mb-3 col-sm-9">
                        <div class="input-group-prepend">
                            <label class="input-group-text" style="width:90px" for="inputGroupSelect01">Tipo</label>
                        </div>
                        <select class="custom-select" id="inputGroupSelect01">
                            <option selected>Choose...</option>
                            <option value="1">One</option>
                            <option value="2">Two</option>
                            <option value="3">Three</option>
                        </select>
                    </div>

                    <div class="input-group mb-3 col-sm-9">
                        <div class="input-group-prepend">
                            <label class="input-group-text" style="width:90px" for="inputGroupSelect01">Estado</label>
                        </div>
                        <select class="custom-select" id="inputGroupSelect01">
                            <option selected>Choose...</option>
                            <option value="1">One</option>
                            <option value="2">Two</option>
                            <option value="3">Three</option>
                        </select>
                    </div>
                </div>

            </div>
        </div>


        <hr class="my-4">

        <br />

        <div class="shadow p-3 mb-5 bg-white rounded">
            <table class="table">
                <thead class="thead-dark" style="background-color:aquamarine">
                    <tr>
                        <th>Número de registro</th>
                        <th>Inscripción</th>
                        <th>Estado</th>
                        <th>Cédula</th>
                        <th>Periodo</th>
                        <th>Tipo</th>
                        <th>Nombre</th>
                        <th>Acción</th>

                    </tr>
                </thead>
                <tbody>
                    
                </tbody>
            </table>

        </div>

        <style>
        body { background-image: url('~/Imagenes/Fondo.png'); }
        </style>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")


</body>
</html>

